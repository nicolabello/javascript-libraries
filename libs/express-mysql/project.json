{
  "root": "libs/express-mysql",
  "sourceRoot": "libs/express-mysql/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/express-mysql/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/express-mysql"
      ],
      "options": {
        "jestConfig": "libs/express-mysql/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/express-mysql",
        "tsConfig": "libs/express-mysql/tsconfig.lib.json",
        "packageJson": "libs/express-mysql/package.json",
        "main": "libs/express-mysql/src/index.ts",
        "assets": [
          "libs/express-mysql/*.md"
        ]
      }
    }
  },
  "tags": []
}
